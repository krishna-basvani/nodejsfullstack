<html>
    <head>
        <link href="../styles.css" rel="stylesheet">
    <head>
<body>
    <table>
        <thead>
            <th>sno</th>
            <th>name</th>
            <th>city</th>
        </thead>
        <%for(let x in data){ %>
        <tr>
            <td><%=data[x].sno%></td>
            <td><%=data[x].name%></td>
            <td><%=data[x].city%></td>
        </tr>
        <% } %>
    </table>
</body>
